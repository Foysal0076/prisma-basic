{
  "name": "prisma_basic",
  "version": "1.0.0",
  "description": "A small grading backend with prisma and postgres ",
  "main": "index.ts",
  "author": "Foysal Ahmed",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "seed": "npx prisma db seed",
    "seed:users": "ts-node ./src/seed-users.ts",
    "test": "jest",
    "postgres:start": "docker-compose up -d",
    "migrate:save": "prisma migrate --experimental save",
    "migrate:up": "prisma migrate --experimental up --auto-approve --create-db",
    "prisma:generate": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "@types/jsonwebtoken": "^9.0.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^9.0.0",
    "prisma": "^4.8.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
